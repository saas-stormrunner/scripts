<script language="javascript">

	window.onload = function(){
		var diskSpaceCheckbox  = document.getElementById("openDiskSpace");
		
		var flag = "null";
		if(flag == "1"){
			diskSpaceCheckbox.checked="checked";
		}
	}
		
	function upload(layout)
	{
		//fix issue #949
		
		var result = "false";
		//check if Authenticated before launch upload applet
		if(result.toString()=="false"){
			var url = "static/mupload.html?context=hprc&layout=" + layout;
			var returnVal = window.showModalDialog(url, "mjc-mupload", 
				"dialogHeight=300px; dialogWidth=600px; status=no; help=no; scroll=no");
			if (returnVal == null || typeof(returnVal) != 'object') {
				// if the returnVal of applet is null or unexpected object, redirect page to home if session doesn't timeout
				var reAuthenticate = "false";
				if(reAuthenticate.toString() == "false"){
					self.location="home";
				}
				else{
					self.location="logout";
				}
			} else if (returnVal.success == true) {
				//redirect to viewName which returned by server. like : self.location='home'
				self.location=returnVal.viewName;
			} else {
				if(returnVal.viewName == null){
					// if the return value of viewName is null, redirect page to home if session doesn't timeout
					var reAuthenticate = "false";
					if(reAuthenticate.toString() == "false"){
						self.location="home";
					}
					else{
						self.location="logout";
					}
				}
				else{ 
					if(returnVal.viewName != "none"){
						self.location=returnVal.viewName;
					}
				}
			}
		}else{
			//if not Authenticated, redirect to login page. 
			alert("session time out");
			self.location='logout';
		}
	}
	
	//select all function
	function selectAll(){ 
	  var s = document.getElementById("selectAllCheckBox");
      var chkArray = document.getElementsByTagName("input");
      for(var i=0;i<chkArray.length;i++){ 
          if(chkArray[i].type=="checkbox" && chkArray[i].name!="DISKSPACE" && chkArray[i].name!="MD5")
     		if (s.checked == true){
          		chkArray[i].checked="checked";
     		}else{
     			chkArray[i].checked=null;
     		}
      }
	} 	
	
    //open and close the function of calculating disk space
  	function cal(){
  		var c = document.getElementById("openDiskSpace");
	    var form = document.getElementById("directory");	   
	    form.action = "/hprc/calculateDiskSpace";
  		form.submit();
	}
</script>

